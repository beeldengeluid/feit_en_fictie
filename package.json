{
  "name": "test-template",
  "private": true,
  "version": "0.1.0",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run build:yaml",
    "build:autoprefixer": "postcss src/static/css/style.css --use autoprefixer --no-map -r",
    "build:css": "npm run build:sass && npm run build:autoprefixer",
    "build:js": "webpack -p",
    "build:sass": "node-sass src/static/css/scss/style.scss src/static/css/style.css",
    "build:yaml": "js-yaml src/static/data/data.yaml > src/static/data/data.json",
    "postinstall": "npm run build",
    "watch:scss": "chokidar 'src/static/css/scss/*.scss' -c 'npm run build:css'",
    "watch:yaml": "chokidar 'src/static/data/*.yaml' -c 'npm run build:yaml'",
    "watch": "npm run watch:scss & npm run watch:yaml & webpack -dw"
  },
  "dependencies": {
    "es6-promise": "^4.2.4",
    "intersection-observer": "^0.5.0",
    "js-yaml": "^3.11.0",
    "lodash": "^4.17.10",
    "loglevel": "^1.6.1",
    "mdn-polyfills": "^5.6.0",
    "regenerator-runtime": "^0.11.1",
    "showdown": "^1.8.6",
    "valenski": "^1.3.0",
    "vue": "^2.5.16",
    "vue-awesome": "^2.3.7",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "chokidar-cli": "^1.2.0",
    "css-loader": "^0.28.11",
    "http-server": "^0.11.1",
    "node-sass": "^4.9.0",
    "postcss": "^6.0.22",
    "postcss-cli": "^5.0.0",
    "vue-loader": "^15.0.9",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.8.1",
    "webpack-cli": "^2.1.3"
  }
}